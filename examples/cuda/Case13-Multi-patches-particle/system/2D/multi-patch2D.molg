#!/usr/bin/python
import sys
sys.path.append('/opt/galamost4/lib')
import molgen

lx=60.0
ly=60.0
lz=0.0

mol0=molgen.Molecule(1)
mol0.setParticleTypes("A")
mol0.setQuaternion()
mol0.setBody(0)

gen=molgen.Generators(lx,ly,lz)
gen.addMolecule(mol0,9360)
gen.setDimension(2)
gen.setMinimumDistance(0.1)
gen.outPutXml("patchyinit")

